{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Absorption Dose",
  "type": "object",
  "properties": {
    "dose": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high"
      ]
    },
    "required_files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "note": {
      "type": "string"
    }
  },
  "required": [
    "dose",
    "required_files"
  ]
}
